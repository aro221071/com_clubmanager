<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset>

              <field name="id" type="text" default="0" label="COM_CLUBMANAGER_FORM_LBL_SETTING_ID"
            readonly="true" class="readonly"
            description="JGLOBAL_FIELD_ID_DESC" /> 

       <field name="name" type="text" size="40" class="inputbox"
                                                    
            label="COM_CLUBMANAGER_FORM_LBL_SETTING_NAME"
            description="COM_CLUBMANAGER_FORM_DESC_SETTING_NAME" 
                                                    
                                                    
                                                     required="true" 
                                                    filter="safehtml" /> 

       <field name="key" type="text" size="40" class="inputbox"
                                                    
            label="COM_CLUBMANAGER_FORM_LBL_SETTING_KEY"
            description="COM_CLUBMANAGER_FORM_DESC_SETTING_KEY" 
                                                    
                                                    
                                                    
                                                    filter="safehtml" /> 

       <field name="datatype" type="text" size="40" class="inputbox"
                                                    
            label="COM_CLUBMANAGER_FORM_LBL_SETTING_DATATYPE"
            description="COM_CLUBMANAGER_FORM_DESC_SETTING_DATATYPE" 
                                                    
                                                    
                                                    
                                                    filter="safehtml" /> 

       <field name="value_numeric" type="text" size="40" class="inputbox"
                                                    
            label="COM_CLUBMANAGER_FORM_LBL_SETTING_VALUE_NUMERIC"
            description="COM_CLUBMANAGER_FORM_DESC_SETTING_VALUE_NUMERIC" 
                                                    
                                                    
                                                    
                                                    filter="safehtml" /> 

       <field name="value_text" type="text" size="40" class="inputbox"
                                                    
            label="COM_CLUBMANAGER_FORM_LBL_SETTING_VALUE_TEXT"
            description="COM_CLUBMANAGER_FORM_DESC_SETTING_VALUE_TEXT" 
                                                    
                                                    
                                                    
                                                    filter="safehtml" /> 

       <field name="value_date" type="calendar" class="inputbox"
        
        
         default="0000-00-00 00:00:00" 
        
            label="COM_CLUBMANAGER_FORM_LBL_SETTING_VALUE_DATE"
            description="COM_CLUBMANAGER_FORM_DESC_SETTING_VALUE_DATE" 
        format="%Y-%m-%d %H:%M:%S"
        filter="safehtml" /> 

       <field name="comment" type="text" size="40" class="inputbox"
                                                    
            label="COM_CLUBMANAGER_FORM_LBL_SETTING_COMMENT"
            description="COM_CLUBMANAGER_FORM_DESC_SETTING_COMMENT" 
                                                    
                                                    
                                                    
                                                    filter="safehtml" /> 

       <field name="created_by" type="createdby" default="" 
            label="COM_CLUBMANAGER_FORM_LBL_SETTING_CREATED_BY"
            description="COM_CLUBMANAGER_FORM_DESC_SETTING_CREATED_BY"  /> 

         <field
        name="state"
        type="list"
        label="JSTATUS"
        description="JFIELD_PUBLISHED_DESC"
        class="inputbox"
        size="1"
        default="1">
        <option value="1">JPUBLISHED</option>
        <option value="0">JUNPUBLISHED</option>
        <option value="2">JARCHIVED</option>
        <option value="-2">JTRASHED</option>
    </field> 

                        <field name="checked_out" type="hidden" filter="unset" />
        <field name="checked_out_time" type="hidden" filter="unset" />

	</fieldset>

</form>
